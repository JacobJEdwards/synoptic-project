{
  "name": "web",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:css": "postcss src/styles/styles.css -o src/views/css/index.css",
    "dev": "nodemon --watch src --exec \"npm run dev:css && ts-node -r tsconfig-paths/register src/app.ts\"",
    "build:css": "postcss src/styles/styles.css -o src/views/css/index.css",
    "compile": "npm run build:css && tsc",
    "postcompile": "cp src/views/index.html dist/views/index.html; mkdir -p dist/views/css/; cp src/views/css/index.css dist/views/css/index.css",
    "build": "npm run compile && npm run postcompile"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "redis": "^4.6.7",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "daisyui": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.3.2",
    "postcss-nested": "^6.0.1",
    "style-loader": "^3.3.3",
    "sugarss": "^4.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "webpack-node-externals": "^3.0.0"
  }
}
